                                                <source media="(min-width: 768px)" sizes="963px" data-srcset="https://static2.cbrimages.com/wordpress/wp-content/uploads/2016/11/batmankillstec29.jpg?q=50&amp;fit=crop&amp;w=963&amp;h=484&amp;dpr=1.5 963w"/>
                                                <source media="(min-width: 481px)" sizes="737px" data-srcset="https://static2.cbrimages.com/wordpress/wp-content/uploads/2016/11/batmankillstec29.jpg?q=50&amp;fit=crop&amp;w=737&amp;h=370&amp;dpr=1.5 737w"/>
                                                <source media="(min-width: 0px)" sizes="450px" data-srcset="https://static2.cbrimages.com/wordpress/wp-content/uploads/2016/11/batmankillstec29.jpg?q=50&amp;fit=crop&amp;w=450&amp;h=226&amp;dpr=1.5 450w"/>
                            <!--[if IE 9]></video><![endif]-->

