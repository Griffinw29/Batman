function adsNinjaDefineSlots(isPageLoad = true, adElementId = null, pageNumber = null){

    // Get GAM ad slots to define.
    var filteredGamAdSlots = null;

    if(null !== isPageLoad && (isPageLoad || (!isPageLoad && !adElementId))){

        filteredGamAdSlots = window.gamAdSlots.filter(function(gamAdSlot){

            return (isPageLoad === gamAdSlot['onPageLoad'] || String(isPageLoad) === gamAdSlot['onPageLoad']);
        });
    }
    else if(null !== pageNumber){

        filteredGamAdSlots = window.gamAdSlots.filter(function(gamAdSlot){

            return (-1 !== (gamAdSlot['adElementId'].indexOf(`-P#${pageNumber}`)));
        });
    }
    else{

        filteredGamAdSlots = window.gamAdSlots.filter(function(gamAdSlot){

            return (adElementId === gamAdSlot['adElementId']);
        });
    }

    var first                     = true;
    var globalCustomTargetingKeys = {
        'Site': true,
        'ContentType': true,
        'Template': true,
        'NetworkCategory': true,
        'SiteCategory': true,
        'PostID': true,
    };

    for(var x in filteredGamAdSlots){

        if(filteredGamAdSlots[x].hasOwnProperty('isInstreamVideo') && filteredGamAdSlots[x]['isInstreamVideo']){

            continue;
        }

        if(first){

            for(var y in filteredGamAdSlots[x]['targeting']){

                if(globalCustomTargetingKeys.hasOwnProperty(y)){

                    window.googletag.pubads().setTargeting(y, filteredGamAdSlots[x]['targeting'][y]);
                }
            }

            first = false;
        }

        window[filteredGamAdSlots[x]['slotReferenceVariableName']] = window.googletag.defineSlot(filteredGamAdSlots[x]['adUnitPath'], filteredGamAdSlots[x]['sizeArray'], filteredGamAdSlots[x]['adElementId']);

        // Protects against falsy value.
        if(!window[filteredGamAdSlots[x]['slotReferenceVariableName']]){

            console.debug(
                '%cAds Debug Log',
                'display: inline-block; color: #fff; background: blue; padding: 1px 4px; border-radius: 3px;',
                `Unable to initialize ad slot: ${filteredGamAdSlots[x]['slotReferenceVariableName']}.`,
            );
            continue;
        }

        for(var y in filteredGamAdSlots[x]['targeting']){

            if(!globalCustomTargetingKeys.hasOwnProperty(y)){

                window[filteredGamAdSlots[x]['slotReferenceVariableName']].setTargeting(y, filteredGamAdSlots[x]['targeting'][y]);
            }
        }

        window[filteredGamAdSlots[x]['slotReferenceVariableName']].addService(window.googletag.pubads());
    }
}

if(typeof window.googletag !== typeof undefined && window.googletag.hasOwnProperty('cmd')){

    window.googletag.cmd.push(function(){

        window.adsNinjaDefineSlots();

        window.googletag.pubads().disableInitialLoad();

        var lazyLoadingOptions = {
            fetchMarginPercent: 225,
            renderMarginPercent: 200,
            mobileScaling: 1.0,
        };

        console.debug(
            '%cAds Debug Log',
            'display: inline-block; color: #fff; background: blue; padding: 1px 4px; border-radius: 3px;',
            'GAM Lazy loading settings.',
            lazyLoadingOptions,
        );

        // Lazy loading.
        window.googletag.pubads().enableLazyLoad(lazyLoadingOptions);

        // Register event handlers to observe lazy loading behavior.
        window.googletag.pubads().addEventListener('slotRequested', function(event){

            console.debug(
                '%cAds Debug Log',
                'display: inline-block; color: #fff; background: blue; padding: 1px 4px; border-radius: 3px;',
                'GPT Event: slotRequested.',
                {
                    slotElementId: event.slot.getSlotElementId(),
                    slotAdUnitPath: event.slot.getAdUnitPath(),
                },
            );
        });

        window.googletag.pubads().addEventListener('slotOnload', function(event){

            var slotElementId = event.slot.getSlotElementId();

            console.debug(
                '%cAds Debug Log',
                'display: inline-block; color: #fff; background: blue; padding: 1px 4px; border-radius: 3px;',
                'GPT Event: slotOnload.',
                {
                    slotElementId: slotElementId,
                    slotAdUnitPath: event.slot.getAdUnitPath(),
                },
            );

            // Send signal for viewable impression.
            if(window.adsNinjaHeaderBiddingManager){

                window.adsNinjaHeaderBiddingManager.handleAdZoneLoadedEvent(slotElementId);
            }
        });

        window.googletag.pubads().addEventListener('slotRenderEnded', function(event){

            var slotElementId  = event.slot.getSlotElementId();
            var slotContainsAd = !event.isEmpty;

            if(slotContainsAd){

                // Do nothing.
                return;
            }

            console.debug(
                '%cAds Debug Log',
                'display: inline-block; color: #fff; background: blue; padding: 1px 4px; border-radius: 3px;',
                'GPT Event: slotRenderEnded',
                {
                    slotElementId: slotElementId,
                    slotAdUnitPath: event.slot.getAdUnitPath(),
                    slotContainsAd: slotContainsAd,
                },
            );

            if(window.adsNinjaHeaderBiddingManager){

                window.adsNinjaHeaderBiddingManager.handleNoFillOrError(slotElementId);
            }
        });

        window.googletag.pubads().addEventListener('impressionViewable', function(event){

            var slot             = event.slot;
            var slotElementId    = event.slot.getSlotElementId();
            var refreshTargeting = slot.getTargeting('refresh');

            // If refresh targeting key is set refresh based on targeting value.
            if(0 !== refreshTargeting.length){

                console.debug(
                    '%cAds Debug Log',
                    'display: inline-block; color: #fff; background: blue; padding: 1px 4px; border-radius: 3px;',
                    `Refreshing ad slot ${slot.getSlotId().getDomId()} in ${refreshTargeting[0]} seconds.`,
                );

                setTimeout(function(){

                    window.googletag.pubads().refresh([slot]);
                }, Number(refreshTargeting[0]) * 1000);

                // TODO: Hold another auction for the slot with A9 and Prebid.
            }

            // Send signal for viewable impression. Sets a timer if one not already created.
            if(window.adsNinjaHeaderBiddingManager){

                window.adsNinjaHeaderBiddingManager.handleAdZoneViewedEvent(slotElementId);
            }
        });

        window.googletag.enableServices();
    });
}
</script>



